    "keywords": ["deployment", "ci"],
    "require": {
        "php": ">=5.4.0",
        "league/shunt": "~2.0",
        "symfony/console": "~2.3",
        "aws/aws-sdk-php": "2.5.x"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Posit\\": "src/"
        }
    },
    "bin": ["bin/posit"],
    "minimum-stability": "dev"
}
